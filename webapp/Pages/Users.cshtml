@page
@model UsersModel
@{
    ViewData["Title"] = "Users page";
}

<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Username</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
  <tbody>
      @for (var i = 0; i < Model.users.Count; i++)
      {
          <tr>
            <th scope="row">@i</th>
            <td>@Model.users[i].UserName</td>
            <td>@Model.users[i].Email</td>
        </tr>
      }
  </tbody>
</table>